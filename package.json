{
  "name": "electron-music-app",
  "version": "1.0.0",
  "description": "react&electron构建网易云音乐",
  "main": "./public/main.js",
  "license": "MIT",
  "author": {
    "name": "RookieHao",
    "email": "chenhao2009_219@126.com",
    "url": "https://github.com/RookieHao"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RookieHao/electron-music-app.git"
  },
  "keywords": [
    "react",
    "typescript",
    "electron",
    "redux"
  ],
  "scripts": {
    "start": "npm run run:render",
    "dev": "npm run run:render",
    "run:main": "tsc && electron .",
    "run:render": "cross-env NODE_ENV=development DEBUG=true webpack-dev-server --color --progress --config  ./config/webpack.render.dev.config.js",
    "build:render": "cross-env NODE_ENV=production DEBUG=false webpack --color --progress --config ./config/webpack.render.prod.config.js",
    "build": "concurrently \"npm run build:render\" \" electron-builder -p nerver \" ",
    "build-win": "rimraf ./dist && concurrently \"npm run build:render\" \"electron-builder -w --x64\" ",
    "build-linux": "npm run build:render && electron-builder -l",
    "build-mac": "npm run build:render && electron-builder -m",
    "build-all": "npm run build:render && electron-builder -mwl"
  },
  "homepage": "https://github.com/RookieHao/electron-music-app.git",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/mini-css-extract-plugin": "^0.2.1",
    "@types/node-notifier": "^5.4.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "css-modules-typescript-loader": "^2.0.4",
    "cssnano": "^4.1.10",
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-notifier": "^5.4.0",
    "node-sass": "^4.12.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "scripty": "^1.9.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "build": {
    "productName": "electron-music-app",
    "appId": "chenhao2009_219@126.com",
    "copyright": "Copyright © 2019 ChenHao",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  }
}
